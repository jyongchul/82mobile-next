{
  "version": 2,
  "rewrites": [
    {
      "source": "/wp-json/:path*",
      "destination": "http://182.162.142.102/wp-json/:path*"
    },
    {
      "source": "/wp-includes/:path*",
      "destination": "http://182.162.142.102/wp-includes/:path*"
    },
    {
      "source": "/wp-content/:path*",
      "destination": "http://182.162.142.102/wp-content/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
